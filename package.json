{
  "name": "portofolio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sass": "sass --style=compressed src/scss/main.scss src/_includes/css/main.css",
    "sass:build": "sass --style=compressed src/scss/main.scss dist/css/main.css",
    "watch:dev": "eleventy --serve",
    "watch:scss": "npm run sass -- --watch",
    "dev": "npm-run-all sass --parallel watch:*",
    "build": "rm -rf dist && npm run sass:build && eleventy"
  },
